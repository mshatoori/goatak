{
    "options": {
        "paths": {
            "root": "",
            "mbtiles": "data",
            "styles": "data",
            "fonts": "data/fonts"
        },
        "formatOptions": {
            "jpeg": {
                "quality": 80
            },
            "webp": {
                "quality": 90
            },
            "png": {
                "palette": true,
                "quality": 90
            }
        },
        "maxScaleFactor": 3,
        "maxSize": 2048,
        "pbfAlias": "pbf",
        "serveAllFonts": true,
        "serveAllStyles": true,
        
        "allowRemoteMarkerIcons": true,
        "allowInlineMarkerImages": true,
        "staticAttributionText": "© OpenMapTiles  © OpenStreetMaps",
        "tileMargin": 0
    },
    "styles": {
        "basic": {
            "style": "basic.json",
            "tilejson": {
              "type": "overlay"
            }
          }
    },
    "data": {
        "iran": {
            "mbtiles": "iran.mbtiles"
        }
    }
}